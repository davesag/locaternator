// see http://www.jshint.com/docs/options/
// be sure to turn off 'devel' before shipping.
{
  "bitwise": true,
  "boss": true,
  "browser": true,
  "camelcase": true,
  "curly": true,
  "devel": true,
  "eqeqeq": true,
  "eqnull": true,
  "es3": false,
  "esnext": true,
  "forin": false,
  "freeze": true,
  "immed": true,
  "indent": 2,
  "jquery": true,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonbsp": true,
  "nonew": true,
  "plusplus": false,
  "quotmark": "double",
  "strict": false,
  "sub": true,
  "trailing": true,
  "undef": true,
  "unused": true,
  "predef": ["jQuery", "console", "async"]
}
